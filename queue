#include<stdio.h>
#include<stdlib.h>
#define MAX 25
int Queue[MAX],rear=-1,front=-1,item;
void insert()
{
if(rear==MAX-1)
  {
 printf("Overflow");
 }
 else
 {
   printf("Enter the element to be inserted  ");
   scanf("%d",&item);
   rear++;
   Queue[rear]=item; 
   }
  }
  void display()
  {
  int i;
   if(top>-1)
   {
     for (i=0;i<=top;i++)
     {
      printf("%d\t",stack[i]);
      }
      }
    else
    {
    printf("empty");
    }
    }
  void pop()
  {
  if(top==-1)
  {
  printf("Empty");
  }
  else
  {
   printf("\n%d is poped out",stack[top]);
   top--;
   
   }
   }
    int main()
    {
    int ch;
    do{
    printf("Enter the choice ");
    scanf("%d",&ch);
    switch(ch)
    {
     case 1:
             insert();
             break;
      case 2:
           delete();
            break;
      case 3:
           display();
            break;
      case 4:
             exit (0);
    }
    }while(1);
    return 0;
    }
